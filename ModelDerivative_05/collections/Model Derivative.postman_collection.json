							"request": {
								"method": "POST",
								"header": [
									{
										"key": "Content-Type",
										"value": "application/json"
									},
									{
										"key": "Authorization",
										"value": "Bearer {{access_token}}"
									}
								],
								"body": {
									"mode": "raw",
									"raw": "{\n\t\"bucketKey\": \"{{ossBucketKey}}\",\n\t\"access\": \"full\",\n\t\"policyKey\": \"transient\"\n}\n"
								},
								"url": {
									"raw": "{{host}}/oss/v2/buckets",
									"host": [
										"{{host}}"
									],
									"path": [
										"oss",
										"v2",
										"buckets"
									]
								},
								"description": "[See documentation](https://forge.autodesk.com/en/docs/data/v2/reference/http/buckets-POST/)"
							},
							"response": []
						},
						{
							"name": "Upload Revit File to OSS",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"var body = JSON.parse(responseBody);",
											"pm.environment.set(\"t5_ossSourceFileObjectKey\", body[\"objectKey\"]);",
											"pm.environment.set(\"t5_ossSourceFileURN\", body[\"objectId\"]);",
											"var encoded_urn=btoa(body[\"objectId\"]);",
											"pm.environment.set(\"t5_ossEncodedSourceFileURN\", encoded_urn);"
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"method": "PUT",
								"header": [
									{
										"key": "Authorization",
										"value": "Bearer {{access_token}}"
									}
								],
								"body": {
									"mode": "file",
									"file": {
										"src": ""
									}
								},
								"url": {
									"raw": "{{host}}/oss/v2/buckets/{{ossBucketKey}}/objects/{{ossSourceFileObjectKey}}",
									"host": [
										"{{host}}"
									],
									"path": [
										"oss",
										"v2",
										"buckets",
										"{{ossBucketKey}}",
										"objects",
										"{{ossSourceFileObjectKey}}"
									]
								},
								"description": "[See documentation](https://forge.autodesk.com/en/docs/data/v2/reference/http/buckets-:bucketKey-objects-:objectName-PUT/)"
							},
							"response": []
						}
