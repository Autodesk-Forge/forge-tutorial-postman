										"key": "Content-Type",
										"name": "Content-Type",
										"value": "application/json",
										"type": "text"
									},
									{
										"key": "Authorization",
										"value": "Bearer {{access_token}}",
										"type": "text"
									},
									{
										"key": "x-ads-force-all",
										"value": "true",
										"type": "text"
									},
									{
										"key": "",
										"value": "",
										"type": "text",
										"disabled": true
									}
								],
								"body": {
									"mode": "raw",
									"raw": "{\n    \"input\": {\n        \"urn\": \"{{t1_ossEncodedSourceFileURN}}\"\n    },\n    \"output\": {\n        \"destination\": {\n            \"region\": \"us\"\n        },\n        \"formats\": [\n            {\n                \"type\": \"obj\"\n            }\n        ]\n    }\n}",
									"options": {
										"raw": {
											"language": "json"
										}
									}
								},
								"url": {
									"raw": "https://developer.api.autodesk.com/modelderivative/v2/designdata/job",
									"protocol": "https",
									"host": [
										"developer",
										"api",
										"autodesk",
										"com"
									],
									"path": [
										"modelderivative",
										"v2",
										"designdata",
										"job"
