									"host": [
										"developer",
										"api",
										"autodesk",
										"com"
									],
									"path": [
										"modelderivative",
										"v2",
										"designdata",
										"{{t1_url_safe_urn_of_source}}",
										"manifest"
									]
								},
								"description": "[See documentation](https://forge.autodesk.com/en/docs/model-derivative/v2/reference/http/urn-manifest-GET/)"
							},
