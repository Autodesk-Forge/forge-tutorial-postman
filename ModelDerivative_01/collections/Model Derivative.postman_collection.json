						{
							"name": "Create a Translation Job",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"var body = JSON.parse(responseBody);",
											"pm.environment.set(\"t1_url_safe_urn_of_source\", body[\"urn\"]);"
										],
