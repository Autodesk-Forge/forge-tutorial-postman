									"value": "{{ListLayersAppFormDataPolicy}}",
									"type": "text"
								},
								{
									"key": "success_action_status",
									"value": "200",
									"type": "text"
								},
								{
									"key": "x-amz-signature",
									"value": "{{ListLayersAppFormDataSignature}}",
									"type": "text"
								},
