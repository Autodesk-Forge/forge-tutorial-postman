							"mode": "formdata",
							"formdata": [
								{
									"key": "key",
									"value": "{{ListLayersAppFormDataKey}}",
									"type": "text"
								},
								{
									"key": "content-type",
									"value": "application/octet-stream",
									"type": "text"
								},
