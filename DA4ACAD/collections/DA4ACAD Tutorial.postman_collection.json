							"raw": "https://developer.api.autodesk.com/da/us-east/v3/appbundles",
							"protocol": "https",
							"host": [
								"developer",
								"api",
								"autodesk",
								"com"
							],
							"path": [
								"da",
								"us-east",
								"v3",
								"appbundles"
							]
